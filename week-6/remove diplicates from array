/* Example: Array of kills by a player in a match (with some duplicates) */
const killLog = [
  { killCount: 1, deathCount: 0, headshots: 1, gnasherKills: 0, explosiveKills: 0, activeReloadBoost: true, jammedWeapon: false, spawnLocation: 1, enemyExecutions: 0 },
  { killCount: 2, deathCount: 0, headshots: 1, gnasherKills: 1, explosiveKills: 0, activeReloadBoost: false, jammedWeapon: false, spawnLocation: 1, enemyExecutions: 0 },
  { killCount: 2, deathCount: 0, headshots: 1, gnasherKills: 1, explosiveKills: 0, activeReloadBoost: false, jammedWeapon: false, spawnLocation: 1, enemyExecutions: 0 },
  { killCount: 3, deathCount: 1, headshots: 2, gnasherKills: 1, explosiveKills: 1, activeReloadBoost: true, jammedWeapon: false, spawnLocation: 2, enemyExecutions: 1 },
];

console.log(removeDuplicates(killLog));

